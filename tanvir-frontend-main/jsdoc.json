{
  "source": {
    "include": [
      "src"
    ],
    "includePattern": ".+\\.js?$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "recurse": true,
    "destination": "./docs/",
    "template": "node_modules/docdash"
  },
  "docdash": {
    "static": true,
    "sort": true,
    "disqus": "",
    "openGraph": {
      "title": "Docdash",
      "type": "website",
      "image": "https://cloud.githubusercontent.com/assets/447956/13398144/4dde7f36-defd-11e5-8909-1a9013302cb9.png",
      "site_name": "Docdash",
      "url": "http://clenemt.github.io/docdash/"
    },
    "meta": {
      "title": "Docdash",
      "description": "A clean, responsive documentation template theme for JSDoc 3.",
      "keyword": "jsdoc, docdash"
    },
    "search": true,
    "collapse": true,
    "typedefs": true,
    "removeQuotes": "none",
    "scripts": []
  }
}